  "panels": [
    {
      "type": "timeseries",
      "title": "CPU (user+system) seconds/sec",
      "targets": [
        {
          "expr": "rate(process_cpu_user_seconds_total[1m]) + rate(process_cpu_system_seconds_total[1m])",
          "legendFormat": "cpu_seconds/s",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 6 }
    },
    {
      "type": "timeseries",
      "title": "Memory RSS (bytes)",
      "targets": [
        { "expr": "process_resident_memory_bytes", "legendFormat": "rss", "refId": "B" }
      ],
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 6 }
    },
    {
      "type": "timeseries",
      "title": "Heap Used (bytes)",
      "targets": [
        { "expr": "nodejs_heap_used_bytes", "legendFormat": "heap_used", "refId": "C" }
      ],
      "gridPos": { "x": 0, "y": 6, "w": 12, "h": 6 }
    },
    {
      "type": "timeseries",
      "title": "Event Loop Lag (seconds)",
      "targets": [
        { "expr": "nodejs_eventloop_lag_seconds", "legendFormat": "evtloop", "refId": "D" }
      ],
      "gridPos": { "x": 12, "y": 6, "w": 12, "h": 6 }
    },
    {
      "type": "timeseries",
      "title": "Active Handles / Active Requests",
      "targets": [
        { "expr": "nodejs_active_handles_total", "legendFormat": "handles", "refId": "E" },
        { "expr": "nodejs_active_requests_total", "legendFormat": "requests", "refId": "F" }
      ],
      "gridPos": { "x": 0, "y": 12, "w": 12, "h": 6 }
    },
    {
      "type": "timeseries",
      "title": "GC Duration (rate seconds/sec)",
      "targets": [
        { "expr": "rate(nodejs_gc_duration_seconds_sum[5m])", "legendFormat": "gc_seconds/s", "refId": "G" }
      ],
      "gridPos": { "x": 12, "y": 12, "w": 12, "h": 6 }
    },
    {
      "type": "timeseries",
      "title": "HTTP Requests /s by route",
      "targets": [
        { "expr": "sum by (route) (rate(http_request_duration_seconds_count{route!~\"^/(_next|_reat)\"}[1m]))", "legendFormat": "{{route}}", "refId": "H" }
      ],
      "gridPos": { "x": 0, "y": 18, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "HTTP p95 per route (5m)",
      "targets": [
        { "expr": "histogram_quantile(0.95, sum by (le, route) (rate(http_request_duration_seconds_bucket{route!~\"^/(_next|_reat)\"}[5m])))", "legendFormat": "p95 {{route}}", "refId": "I" }
      ],
      "gridPos": { "x": 12, "y": 18, "w": 12, "h": 8 }
    }
  ],
  "refresh": "5s",
